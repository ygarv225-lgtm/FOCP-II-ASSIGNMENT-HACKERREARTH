#include <iostream>
#include <string>
using namespace std;

int main() {
    string s;
    cin >> s;

    // Loop through each character in the string
    for (int i = 0; i < s.length(); i++) {
        // Check if the character is lowercase
        if (s[i] >= 'a' && s[i] <= 'z') {
            s[i] = s[i] - 32; // Convert to Uppercase
        } 
        // Check if the character is uppercase
        else if (s[i] >= 'A' && s[i] <= 'Z') {
            s[i] = s[i] + 32; // Convert to Lowercase
        }
    }

    cout << s << endl;
    return 0;
}
